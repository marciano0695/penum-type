{
    "name": "marcionunes/penum-type",
    "description": "For laravel. Convert your php enum to typescript",
    "type": "library",
    "authors": [
        {
            "name": "MÃ¡rcio Nunes",
            "email": "marcionunesgouveia1@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.24",
        "pestphp/pest": "^3.8",
        "orchestra/testbench": "^10.4"
    },
    "autoload": {
        "psr-4": {
            "Marcionunes\\PenumType\\": "src/",
            "Tests\\": "tests/",
            "Tests\\App\\": "tests/app/",
            "Tests\\App\\Enums\\": "tests/app/Enums/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Marcionunes\\PenumType\\PenumTypeServiceProvider"
        ]
      }
    },
    "scripts": {
        "test": "./vendor/bin/pest",
        "pint": "./vendor/bin/pint"
    }
}
