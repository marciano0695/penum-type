{
    "name": "marcionunes/penum-type",
    "description": "For laravel. Convert your php enum to typescript .d.ts",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "penum-type",
        "laravel",
        "php-typescript"
    ],
    "homepage": "https://github.com/marciano0695/penum-type",
    "authors": [
        {
            "name": "MÃ¡rcio Nunes",
            "email": "marcionunesgouveia1@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.24",
        "pestphp/pest": "^3.8",
        "orchestra/testbench": "^10.4"
    },
    "autoload": {
        "psr-4": {
            "Marcionunes\\PenumType\\": "src/",
            "Tests\\": "tests/",
            "Tests\\App\\": "tests/app/",
            "Tests\\App\\Enums\\": "tests/app/Enums/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Marcionunes\\PenumType\\PenumTypeServiceProvider"
        ]
      }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
